<%= render template: '_patterns/00-atoms/00-meta/doctype' %>
<%= render template: '_patterns/00-atoms/00-meta/html' %>
<head>
<%= render template: '_patterns/00-atoms/00-meta/meta' %>
<%= render template: '_patterns/00-atoms/00-meta/title' %>
<%= render template: '_patterns/00-atoms/00-meta/script-head' %>
<%= render template: '_patterns/00-atoms/00-meta/links' %>

<%= opensearch_description_tag application_name, opensearch_catalog_url(:format => 'xml') %>
<%= favicon_link_tag 'favicon.ico' %>
<%= stylesheet_link_tag "application", media: "all" %>
<%= javascript_include_tag "application" %>
<%= csrf_meta_tags %>
<%= content_for(:head) %>
</head>
<%= render template: '_patterns/00-atoms/00-meta/body' %>
<%= render template: '_patterns/00-atoms/00-meta/svg-icons-body' %>
<%= render :partial => 'shared/header_navbar' %>

<%= render partial: 'shared/ajax_modal' %>

<div id="main-container" class="container">
<%= render :partial=>'/flash_msg', layout: 'shared/flash_messages' %>
<div class="row"><%= yield %></div>
</div>

<%= render :partial => 'shared/footer' %>
</body>
</html>